{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "rules": {
        "max-line-length": false,
        "member-access": false,
        "no-console": false,
        "object-literal-sort-keys": false,
        "ordered-imports": false,
        "quotemark": false,
        "trailing-comma": false
    }
}
